<application>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*.pyc;*.pyo;*.rbc;*.yarb;*~;.DS_Store;.git;.hg;.svn;CVS;__pycache__;_svn" />
    <extensionMap>
      <mapping pattern=".bablerc" type="ECMAScript 6" />
      <mapping ext="2" type="PHP" />
      <mapping ext="sock" type="PLAIN_TEXT" />
      <mapping ext="-php" type="XHTML" />
      <removed_mapping ext="ignore" approved="true" type="IgnoreLang file" />
      <removed_mapping ext="log" approved="true" type="PLAIN_TEXT" />
      <removed_mapping pattern="style.css" approved="true" type="AUTO_DETECTED" />
      <removed_mapping pattern="style.css" approved="true" type="CSS" />
    </extensionMap>
  </component>
</application>